{
  "name": "codex",
  "version": "0.8.0-rc2",
  "private": true,
  "description": "ui for codex api",
  "scripts": {
    "build": "vue-cli-service build --modern",
    "dev": "vue-cli-service build --watch",
    "fix": "vue-cli-service lint --cache --fix --ext .cjs,.mjs,.js,.json,.md,.vue,.yaml",
    "lint": "vue-cli-service lint --cache --ext .cjs,.mjs,.js,.json,.md,.vue,.yaml",
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.19",
    "eslint-plugin-yaml": "^0.5.0",
    "filesize": "^8.0.5",
    "normalize.css": "^8.0.1",
    "reconnecting-websocket": "^4.4.0",
    "vue": "^2.6.14",
    "vue-meta": "^2.4.0",
    "vue-native-websocket": "^2.0.15",
    "vue-router": "^3.5.3",
    "vue2-filters": "^0.14.0",
    "vuetify": "^2.5.9",
    "vuex": "^3.6.2",
    "vuex-cache": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@fsouza/prettierd": "^0.18.1",
    "@mdi/font": "^6.4.95",
    "@mdi/js": "^6.4.95",
    "@vue/cli": "^5.0.0-rc.1",
    "@vue/cli-plugin-babel": "^5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "^5.0.0-beta.7",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.1",
    "@vue/cli-plugin-vuex": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.1.1",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.4.6",
    "eslint": "^8.5.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^40.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "eslint-plugin-vue-scoped-css": "^2.0.0",
    "eslint_d": "^11.1.1",
    "prettier": "^2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.4",
    "typeface-roboto": "^1.1.13",
    "vue-cli-plugin-vuetify": "^2.0.8",
    "vue-eslint-parser": "^8.0.0",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "webpack": "^5.65",
    "webpack-bundle-tracker": "^1.4.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/../test-results/jest/coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue2-jest"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "tabWidth": 2
  }
}
