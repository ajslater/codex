{
  "name": "codex",
  "version": "0.13.0-a0",
  "private": true,
  "description": "ui for codex api",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "fix": "eslint --fix --ext .cjs,.mjs,.js,.json,.md,.vue,.yaml . && prettier . --write",
    "lint": "eslint --ext .cjs,.mjs,.js,.json,.md,.vue,.yaml . && prettier --check .",
    "test:unit": "vue-cli-service test:unit"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.19",
    "eslint-import-resolver-alias": "^1.1.2",
    "humanize": "^0.0.9",
    "vue": "^2.6.14",
    "vue-meta": "^2.4.0",
    "vue-native-websocket": "^2.0.15",
    "vue-pdf-embed": "^1.1.1",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.8",
    "vuex": "^3.6.2",
    "vuex-cache": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@fsouza/prettierd": "^0.21.1",
    "@mdi/font": "^7.0.96",
    "@mdi/js": "^7.0.96",
    "@types/node": "^18.7.13",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0",
    "babel-jest": "^27.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^43.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-yaml": "^0.5.0",
    "eslint_d": "^12.0.0",
    "esm": "^3.2.25",
    "prettier": "^2.2.1",
    "prettier-plugin-package": "^1.3.0",
    "sass": "^1.43.4",
    "sass-loader": "^13.0",
    "typeface-roboto": "^1.1.13",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-static-copy": "^0.7.0",
    "vue-eslint-parser": "^9.0.2",
    "vuetify-loader": "^1.6.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/../test-results/jest/coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue2-jest"
    }
  }
}
