{ {% load static %}
  "name": "Codex",
  "short_name": "Codex",
  "description": "Comic book reader",
  "scope": "{{ script_prefix }}",
  "start_url" : "{{ script_prefix }}",
   "icons": [
    {
      "src": "{% static 'img/favicon-128.webp' %}",
      "sizes": "128x128",
      "type": "image/webp"
    },
    {
      "src": "{% static 'img/favicon-192.webp' %}",
      "sizes": "192x192",
      "type": "image/webp"
    },
    {
      "src": "{% static 'img/favicon-512.webp' %}",
      "sizes": "512x512",
      "type": "image/webp"
    }
  ],
  "display": "standalone",
  "lang": "en-US"
{% comment %}
  "dir": "auto",
  "orientation": "any",
  "background_color": "#000000",
  "theme_color": "#cc7b19",
  "status_bar": "default",
{% endcomment %}
}
